		{% endblock %}

		{% block javascripts %}
			<script src="https://www.paypal.com/sdk/js?client-id=Aen7I84ewIdzk-GGC22SKK-Fmx-woGpJrrPAroTrgwfMC1fc5Xgvr0DVyw_EhkoxlUjLT54vwXL171kq&currency=EUR"></script>

			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% include "navbar/navbar.html.twig" %}

		<section class="content"> {% block body %}{% endblock %}
			</section>
			{% include "footer/footer.html.twig" %}
		</body>
	</html>
